{
  "name": "supa-vercel-stack-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "vercel-build": "prisma generate && react-router build",
    "db:prepare-migration": "prisma migrate dev --create-only --skip-seed",
    "db:deploy-migration": "prisma migrate deploy",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": " npm install && node ./scripts/init-claude.js && run-s db:reset",
    "setup:seed": "prisma db seed",
    "start": "vite preview",
    "start:e2e": "vite dev --port 8811",
    "start:ci": "dotenv -- vite preview",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 'npx playwright test --ui'",
    "test:e2e:run": "dotenv -- cross-env PORT=8811 start-server-and-test start:e2e http://localhost:8811 'npx playwright test --reporter=list'",
    "test:e2e:run:ui": "dotenv -- cross-env PORT=8811 start-server-and-test start:e2e http://localhost:8811 'npx playwright test --ui'",
    "typecheck": "tsc -b && tsc -b test",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "postinstall": "prisma generate",
    "db:reset": "npm run db:reset:supabase && npm run db:reset:prisma",
    "db:reset:supabase": "npx supabase db reset",
    "db:reset:prisma": "dotenv -- npx prisma db pull && dotenv -- npx prisma generate && npm run db:seed",
    "db:seed": "dotenv -- tsx app/database/seed.server.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@react-router/fs-routes": "^7.9.6",
    "@react-router/node": "^7.0.0",
    "@supabase/supabase-js": "^2.39.1",
    "autoprefixer": "^10.4.16",
    "cookie": "^0.6.0",
    "i18next": "^24.0.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.2",
    "isbot": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router": "^7.0.0",
    "react-zorm": "^0.9.0",
    "remix-i18next": "^7.4.2",
    "tailwind-merge": "^2.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.0.37",
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.56.1",
    "@react-router/dev": "^7.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/i18next-fs-backend": "^1.1.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vercel/react-router": "^1.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "happy-dom": "^12.10.3",
    "msw": "^2.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.1",
    "prisma": "^5.7.1",
    "start-server-and-test": "^2.0.3",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.4.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.1.0"
  },
  "overrides": {
    "msw": {
      "typescript": "$typescript"
    }
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "prisma": {
    "schema": "app/database/schema.prisma",
    "seed": "tsx app/database/seed.server.ts"
  }
}
